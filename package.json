{
  "name": "@claimiq/system",
  "version": "1.0.0",
  "description": "ClaimIQ AI-powered Insurance Denial Recovery System",
  "private": true,
  "workspaces": [
    "be/services/*",
    "be/services/workflows/*",
    "ui"
  ],
  "scripts": {
    "// Build Scripts": "",
    "build": "npm run build:be && npm run build:ui",
    "build:be": "tsc",
    "build:ui": "cd ui && npm run build",
    "build:api": "cd be/services/api && tsc",
    "build:workflows": "cd be/services/workflows/claim-processor-workflow && tsc",
    "clean": "rm -rf dist be/services/*/dist be/services/*/*/dist .serverless be/services/*/.serverless be/services/*/*/.serverless ui/dist ui/build",
    
    "// Code Quality": "",
    "lint": "npm run lint:be && npm run lint:ui",
    "lint:be": "eslint be/services/**/*.ts be/libs/**/*.ts --fix",
    "lint:ui": "cd ui && npm run lint",
    "format": "prettier --write \"**/*.{ts,js,json,yml,yaml,md}\"",
    "type-check": "npm run type-check:be && npm run type-check:ui",
    "type-check:be": "tsc --noEmit",
    "type-check:ui": "cd ui && npm run type-check",
    "validate": "npm run type-check && npm run lint",
    
    "// Testing": "",
    "test": "npm run test:be && npm run test:ui",
    "test:be": "jest be/",
    "test:ui": "cd ui && npm run test",
    "test:api": "jest be/services/api",
    "test:workflows": "jest be/services/workflows",
    "test:integration": "jest tests/integration",
    "test:infrastructure": "jest tests/infrastructure",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    
    "// Development": "",
    "dev": "npm run dev:be",
    "dev:be": "npm run start:local",
    "dev:ui": "cd ui && npm run dev",
    "dev:full": "concurrently \"npm run dev:be\" \"npm run dev:ui\"",
    
    "// Deployment": "",
    "deploy": "npm run validate && npm run deploy:be",
    "deploy:be": "npm run deploy:services",
    "deploy:services": "npm run deploy:api && npm run deploy:workflows",
    "deploy:api": "cd be/services/api && serverless deploy --stage ${STAGE:-dev}",
    "deploy:workflows": "cd be/services/workflows/claim-processor-workflow && serverless deploy --stage ${STAGE:-dev}",
    "deploy:full": "./scripts/deploy-json.sh",
    "deploy:full:dev": "./scripts/deploy-json.sh dev",
    "deploy:full:staging": "./scripts/deploy-json.sh staging",
    "deploy:full:prod": "./scripts/deploy-json.sh prod",
    
    "// Environment Management": "",
    "env:generate": "./scripts/generate-env-json.sh",
    "env:dev": "./scripts/generate-env-json.sh dev",
    "env:staging": "./scripts/generate-env-json.sh staging",
    "env:prod": "./scripts/generate-env-json.sh prod",
    
    "// Local Development": "",
    "start:local": "./scripts/start-local-json.sh",
    "offline:api": "cd be/services/api && serverless offline start",
    
    "// Utilities": "",
    "package:api": "cd be/services/api && serverless package",
    "package:workflows": "cd be/services/workflows/claim-processor-workflow && serverless package",
    "logs:api": "cd be/services/api && serverless logs",
    "logs:workflows": "cd be/services/workflows/claim-processor-workflow && serverless logs",
    "info:api": "cd be/services/api && serverless info",
    "info:workflows": "cd be/services/workflows/claim-processor-workflow && serverless info",
    "remove:api": "cd be/services/api && serverless remove",
    "remove:workflows": "cd be/services/workflows/claim-processor-workflow && serverless remove"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.658.1",
    "@aws-sdk/client-dynamodb": "^3.658.1",
    "@aws-sdk/client-rds-data": "^3.658.1",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/client-sfn": "^3.658.1",
    "@aws-sdk/lib-dynamodb": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "data-api-client": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "serverless": "^3.40.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^14.4.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-python-requirements": "^6.1.2",
    "serverless-s3-local": "^0.8.5",
    "serverless-step-functions": "^3.23.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "healthcare",
    "insurance",
    "ai",
    "typescript",
    "claimiq"
  ],
  "author": "ClaimIQ Team",
  "license": "PROPRIETARY"
}
